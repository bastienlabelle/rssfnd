{
	"name": "RSSFND",
	"version": "0.1",
	"description": "Small extension to find feeds on a webpage",
	"browser_action": {
		"default_icon": "icon48.png",
		"popup": "popup.html"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": ["tabs"],
	"content_scripts":[{
		"matches": ["<all_urls>"],
		"js": ["dom.js"]
	 }]
}